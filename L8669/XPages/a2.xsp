<?xml version="1.0" encoding="UTF-8"?>
<xp:view xmlns:xp="http://www.ibm.com/xsp/core"
	xmlns:xc="http://www.ibm.com/xsp/custom">
	<xp:this.resources>
		<xp:styleSheet href="/docs.min.css"></xp:styleSheet>
	</xp:this.resources>
	<xc:ccLayoutBootstrap>
		<xp:this.facets>
			<xp:panel xp:key="facetMiddle">

				<div class="bs-callout bs-callout-danger">
					<h4>Component dependencies</h4>
					<p>
						Requires jQuery,
						<a href="http://getbootstrap.com/"
							target="_blank">
							Bootsrap 3.x
						</a>
						CSS and JavaScript files. Depends on Bootstrap
						<a
							href="http://getbootstrap.com/javascript/#dropdowns"
							target="_blank">
							Dropdown component
						</a>
						and
						<a href="http://daneden.github.io/animate.css/"
							target="_blank">
							animate.css
						</a>
						if want to use animations.
					</p>
				</div>
				<xp:panel styleClass="bs-callout bs-callout-info">
					<xp:text escape="true" id="computedField1"
						value="#{javascript:view.getPageName()}"
						style="font-size:14pt;font-weight:bold">
					</xp:text>
				</xp:panel>


				<br />
				<xp:text id="computedField2" escape="false"
					value="#{javascript:getList2()}">
				</xp:text>
				<br />
				<br />
				<br />
				<xp:text id="computedField3" escape="false"
					value="#{javascript:getListBoton()}">
				</xp:text>
				<br />
				<br />
				<br />
				<xp:text id="computedField4" escape="false"
					value="#{javascript:getList()}">
				</xp:text>
				<br />
				<br />
				<br />
				<xp:text id="computedField5" escape="false"
					value="#{javascript:getListOriginal()}">
				</xp:text>

			</xp:panel>
		</xp:this.facets>
	</xc:ccLayoutBootstrap>


</xp:view>
